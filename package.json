{
  "name": "trino-sql-parser",
  "version": "1.0.0",
  "description": "Sql parser for trino built with antlr4",
  "source": "src/index.js",
  "main": "dist/main.mjs",
  "context": "node",
  "scripts": {
    "watch": "parcel watch",
    "build": "npm run build:parser && npm run build:dist",
    "build:dist": "parcel build",
    "build:parser": "antlr -Dlanguage=JavaScript lib/TrinoLexer.g4 lib/TrinoParser.g4",
    "test": "ava"
  },
  "keywords": [
    "trino",
    "sql",
    "parser",
    "antlr",
    "antlr4"
  ],
  "author": "Martin Fracker <martin.frackerjr@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@swc/helpers": "^0.4.11",
    "antlr4": "^4.11.0"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.3",
    "ava": "^4.3.3",
    "parcel": "^2.7.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ]
}
